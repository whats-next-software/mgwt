<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder">
    <ui:with field="appearance" type="com.googlecode.mgwt.ui.client.widget.button.ImageButtonAppearance" />
	<div ui:field="button" class="{appearance.css.button}">
	  <div ui:field="image" class="{appearance.css.image}"/>
	  <div ui:field="text" class="{appearance.css.text}"/>
	</div>
</ui:UiBinder>